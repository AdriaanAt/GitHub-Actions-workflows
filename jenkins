// import

@Library("QualfonReusablePipeline@2.0.2")
import com.qualfon.*

def se = new setEnvironment();

def envType = "debug"
    if(env.BRANCH_NAME.startsWith("release") )
        {envType = "qa"}
    if(env.BRANCH_NAME == "master")
        {envType = "prod"}
    if(env.BRANCH_NAME == "develop")
        {envType = "dev"}
    if(env.BRANCH_NAME == "uat")
        {envType = "uat"}

pipeline{
        agent any
    environment{
        //Parameters that are built from values currently available:
        cleanBranchName = env.BRANCH_NAME.replace('/','')
        appName = env.JOB_NAME.substring(0,env.JOB_NAME.indexOf('/'))
        iisAppName = ""
        projectName = ""
        //Parameters that are built from the application's CICD.JSON file:
        dest = ""
        database = ""
        databaseServer = ""
        prodDatabaseServer = ""
        appType = ""
        iterativeFolder = ""
        iterativeCollection = ""
    }
